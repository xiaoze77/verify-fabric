{
  "name": "verify-fabric",
  "version": "1.5.0",
  "description": "集合eslint、stylelint、prettier检查",
  "main": "./dist/index",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "prettier",
    "eslint",
    "stylelint",
    "react"
  ],
  "scripts": {
    "build": "yarn run clean && tsc --skipLibCheck",
    "release": "standard-version",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xz-77/verify-fabric.git"
  },
  "author": "xz",
  "bugs": {
    "url": "https://github.com/xz-77/verify-fabric/issues"
  },
  "homepage": "https://github.com/xz-77/verify-fabric#readme",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^43.0.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-order": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/node": "^18.0.3",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^7.0.0",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.0",
    "typescript": "^4.5.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
