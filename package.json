{
  "name": "verify-fabric",
  "version": "1.3.0",
  "description": "集合eslint、stylelint、prettier检查",
  "main": "./dist/index",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "prettier",
    "eslint",
    "stylelint",
    "react"
  ],
  "scripts": {
    "build": "yarn run clean && tsc --skipLibCheck",
    "release": "standard-version",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xz-77/verify-fabric.git"
  },
  "author": "xz",
  "bugs": {
    "url": "https://github.com/xz-77/verify-fabric/issues"
  },
  "homepage": "https://github.com/xz-77/verify-fabric#readme",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-compat": "^3.13.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^36.0.0",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.4.0",
    "stylelint-order": "^4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^15.0.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^7.0.0",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.0",
    "typescript": "^4.5.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
