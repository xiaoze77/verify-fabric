{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "declaration": false,
    "removeComments": true
  }
}
